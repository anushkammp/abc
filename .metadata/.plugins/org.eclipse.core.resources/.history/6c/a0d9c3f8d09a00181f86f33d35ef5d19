		li.add(new Book(123,"Adv Java","Oracle",34));
		li.add(new Book(1234,"Adv Java","Oracle",1234));
		
		System.out.println("No of books:"+li.size());
//		System.out.println(li);
		for(Book b:li)
			System.out.println(b);
		
