package com.exception;

public class MyException extends Exception
{
	private String msg;
	
	public MyException() {
		// TODO Auto-generated constructor stub
		msg="Got exception";
	}
	
	public MyException(String msg)
	{
		super(msg);
		this.msg=msg;
	}
	
	public void showCause()
	{
		System.out.println("exception..........");
	}

}
